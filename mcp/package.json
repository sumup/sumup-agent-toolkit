{
  "name": "@sumup/mcp",
  "version": "0.0.5",
  "description": "MCP server for interacting with SumUp",
  "homepage": "https://github.com/sumup/sumup-agent-toolkit",
  "license": "Apache-2.0",
  "author": "SumUp <support@sumup.com> (https://sumup.com/)",
  "main": "dist/index.mjs",
  "bin": {
    "mcp": "dist/index.mjs"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumup/sumup-agent-toolkit.git"
  },
  "scripts": {
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "inspect": "npx @modelcontextprotocol/inspector node dist/index.mjs",
    "build": "rslib build",
    "build:watch": "rslib build --watch",
    "changeset": "changeset",
    "prerelease": "npm run build",
    "release": "changeset publish",
    "version": "changeset version",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "bugs": {
    "url": "https://github.com/sumup/sumup-agent-toolkit/issues"
  },
  "keywords": [
    "sumup",
    "payments",
    "mcp",
    "mcp-server",
    "modelcontextprotocol"
  ],
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.8.0",
    "@sumup/agent-toolkit": "^0.0.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.4",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.28.1",
    "@rslib/core": "^0.11.2",
    "@types/node": "^22.13.14",
    "rsbuild-plugin-publint": "^0.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
