{
  "name": "@sumup/agent-toolkit",
  "version": "0.2.0",
  "homepage": "https://github.com/sumup/sumup-agent-toolkit",
  "license": "Apache-2.0",
  "author": "SumUp <support@sumup.com> (https://sumup.com/)",
  "type": "commonjs",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "tsup",
    "postbuild": "publint",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumup/sumup-agent-toolkit.git"
  },
  "exports": {
    "./langchain": {
      "types": "./dist/langchain/index.d.ts",
      "require": "./dist/langchain/index.js",
      "import": "./dist/langchain/index.mjs"
    },
    "./openai": {
      "types": "./dist/openai/index.d.ts",
      "require": "./dist/openai/index.js",
      "import": "./dist/openai/index.mjs"
    },
    "./mcp": {
      "types": "./dist/mcp/index.d.ts",
      "require": "./dist/mcp/index.js",
      "import": "./dist/mcp/index.mjs"
    },
    "./ai": {
      "types": "./dist/ai/index.d.ts",
      "require": "./dist/ai/index.js",
      "import": "./dist/ai/index.mjs"
    }
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@langchain/core": "^1.0.6",
    "@modelcontextprotocol/sdk": "^1.22.0",
    "@sumup/sdk": "^0.0.6",
    "ai": "^5.0.98",
    "openai": "^4.96.0",
    "zod": "^4.1.12",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.15.17",
    "globals": "^16.4.0",
    "publint": "^0.3.15",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json",
    "dist/**/*"
  ]
}
